<div id="carousel-web-dev" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-web-dev" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-web-dev" data-slide-to="1"></li>
    <li data-target="#carousel-web-dev" data-slide-to="2"></li>
    <li data-target="#carousel-web-dev" data-slide-to="3"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <%= image_tag("inclustaff/home.png", class: "img-responsive") %>
      <div class="carousel-caption">
        IncluStaff.com
      </div>
    </div>
    <div class="item">
      <%= image_tag("inclustaff/chart-1.png", class: "img-responsive") %>
      <div class="carousel-caption">
      </div>
    </div>
    <div class="item">
      <%= image_tag("inclustaff/activity.png", class: "img-responsive") %>
      <div class="carousel-caption">
      </div>
    </div>
    <div class="item">
      <%= image_tag("inclustaff/orders-table.png", class: "img-responsive") %>
      <div class="carousel-caption">
      </div>
    </div>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#carousel-web-dev" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#carousel-web-dev" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>